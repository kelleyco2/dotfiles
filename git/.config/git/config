[alias]
    gbr = !git branch | grep -v \"master\" | xargs git branch -D
    ac = !git add . && git commit -m
    cob = !git checkout -b
    ammend = !git commit --amend -m
    c = !git checkout
    pwl = !git push --force-with-lease
    quickfix = !git add . && git commit --amend --no-edit
	reset = !git reset --soft HEAD~1
